<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:s="urn:import:com.googlecode.mgwt.ui.client.widget.panel.scroll"
	xmlns:b="urn:import:com.googlecode.mgwt.ui.client.widget.button"
	xmlns:v="urn:import:com.hiep.supermusic.client.view" xmlns:r="urn:import:com.googlecode.mgwt.ui.client.widget.input.radio">
	<ui:style>
	.homeView {
		position: relative;
	}
	.buttonView {
		border-radius: 20px !important; 
	}
	.container {
		background: transparent;
	}
	.content-primary {
		text-align: center !important;
	}
	.searchPanel {
		width: 100%;
	}
	</ui:style>
	<g:FlowPanel styleName="{style.homeView}" width="100%">
		<g:VerticalPanel width="100%" ui:field="contentPanel" spacing="5" addStyleNames="{style.container}"></g:VerticalPanel>
	</g:FlowPanel>
</ui:UiBinder> 